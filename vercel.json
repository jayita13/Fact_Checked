{
  "version": 2,
  "builds": [
    {
      "src": "./",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/*": {
      "runtime": "vercel-python@0.24.0"
    }
  },
  "env": {
    "MY_ENV_VAR": "value"
  },
  "install": {
    "include": ["requirements.txt"],
    "commands": [
      "python3 -m venv myenv",
      "source myenv/bin/activate",
      "pip install -r requirements.txt"
    ]
  }
}
